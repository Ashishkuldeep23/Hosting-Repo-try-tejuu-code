 {
  "version": 2,
  "builds": [
    {
      "src": "./routes.js",
      "use": "@vercel/node"
    }
  ],
 
  "routes": [
    { "src": "^/static/(.*)", "dest": "/static/$1" },
    { "src": ".*", "dest": "/index.html" }
  ],
  "headers": [
    { "source": "/(.*)", "headers": [{ "key": "Cache-Control", "value": "public, max-age=3600" }] }
  ]


}

