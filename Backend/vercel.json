 {
  "version": 2,
  "builds": [
    {
      "src": "./routes.js",
      "use": "@vercel/node"
    }
  ],
 

  
  "rewrites": [
    { "source": "^/static/(.*)", "destination": "/static/$1" },
    { "source": "/(.*)", "destination": "/index.html" }
  ],
  "headers": [
    { "source": "/(.*)", "headers": [{ "key": "Cache-Control", "value": "public, max-age=3600" }] }
  ]





}

